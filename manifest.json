{
  "manifest_version": 3,
  "name": "tedeh.net chrome extension",
  "description": "My personal selection of necessities",
  "version": "1.0",
  "permissions": [
    "webNavigation",
    "webRequest",
    "scripting",
    "storage",
    "tabs"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.flashback.org/*"],
      "js": ["flashback.js"]
    },
    {
      "matches": ["https://www.reddit.com/*"],
      "js": ["reddit.js"]
    },
    {
      "matches": ["https://twitter.com/*"],
      "js": ["twitter.js"]
    },
    {
      "matches": ["https://www.fazwaz.com/*"],
      "js": ["fazwaz_unfucker.js"]
    },
    {
      "matches": ["https://*/*"],
      "js": [
        "twitter_link_killer.js",
        "amp_link_killer.js",
        "overflow_hidden_killer.js",
        "meta_f_unabuser.js",
        "copy_unabuser.js",
        "utm_link_killer.js"
      ]
    }
  ]
}
